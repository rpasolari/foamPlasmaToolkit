FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   nonuniform List<scalar> 4(0 0 1 1);

boundaryField
{
    back
    {
        type            empty;
    }
    down
    {
        type            zeroGradient;
    }
    up
    {
        type            zeroGradient;
    }
    left
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    right
    {
        type            zeroGradient;
    }
}


