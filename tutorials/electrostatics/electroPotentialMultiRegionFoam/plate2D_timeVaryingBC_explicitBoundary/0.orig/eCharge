FoamFile
{
    format      ascii;
    class       volScalarField;
    object      eCharge;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    ".*"
    {
        type    calculated;
        value   $internalField;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}
