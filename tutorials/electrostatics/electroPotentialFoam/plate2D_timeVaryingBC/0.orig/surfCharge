FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      surfCharge;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type    fixedValue;
        value   uniform 0;
    }

    walls
    {
        type    fixedValue;
        value   uniform 0;
    }

    frontAndBack
    {
        type    empty;
    }
}

