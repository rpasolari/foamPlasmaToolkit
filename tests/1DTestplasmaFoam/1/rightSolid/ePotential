FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "1/rightSolid";
    object      ePotential;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   nonuniform List<scalar> 5(259.167 201.797 144.426 87.0557 29.6852);

boundaryField
{
    bottomTopBackFront
    {
        type            empty;
    }
    right
    {
        type            fixedValue;
        value           uniform 1;
    }
    rightSolid_to_leftFluid
    {
        type            coupledElectricPotential;
        useImplicit     true;
        refValue        uniform 260.067;
        refGradient     uniform 112.941;
        valueFraction   uniform 0.5;
        source          uniform 1.65134e-09;
        value           uniform 287.852;
        surfChargeNbr   surfCharge;
        logInterval     -1;
        writePrecision  6;
        updateHeader    1;
        writeToFile     0;
        useUserTime     1;
    }
}


