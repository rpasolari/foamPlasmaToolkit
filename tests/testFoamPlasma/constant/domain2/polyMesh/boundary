(
    back
    {
        type            empty;
        inGroups        1(empty);
        nFaces          2;
        startFace       1;
    }
    down
    {
        type            patch;
        nFaces          1;
        startFace       3;
    }
    up
    {
        type            patch;
        nFaces          1;
        startFace       4;
    }
    front
    {
        type            empty;
        inGroups        1(empty);
        nFaces          2;
        startFace       5;
    }
    right
    {
        type            patch;
        nFaces          2;
        startFace       7;
    }
    domain2_to_domain1
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          2;
        startFace       9;
        sampleMode      nearestPatchFace;
        sampleRegion    domain1;
        samplePatch     domain1_to_domain2;
    }
)

