FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/leftFluid";
    object      ePotential;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 2 -3 0 0 -1 0 ];

internalField   uniform 0;

boundaryField
{
    backFront
    {
        type            empty;
    }
    bottom
    {
        type            zeroGradient;
        value           nonuniform List<scalar> 0();
    }
    top
    {
        type            zeroGradient;
        value           nonuniform List<scalar> 0();
    }
    left
    {
        type            fixedValue;
        value           uniform 10;
    }
    leftFluid_to_rightSolid
    {
        type            coupledElectricPotential;
        value           $internalField;
        surfCharge      surfCharge;
        surfChargeNbr   none;
        useImplicit     true;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0;
    }
}


