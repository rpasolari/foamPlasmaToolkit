FoamFile
{
    format          ascii;
    class           dictionary;
    location        "system/domain1";
    object          fvSchemes;
}

pi              3.14159;

epsilonRDomain1 1;

epsilonRDomain2 1;

Amp             0.0001;

freq_x          8;

freq_y          8;

meshSize        "2x2";

distortMesh     false;

dirichletValueLeft 1;

dirichletValueRight 0;

sigmaS          0;

laplacianScheme Gauss linear corrected;

solver          GAMG;

smoother        DIC;

preconditioner  none;

tolerance       1e-08;

relTol          0;

nNonOrthogonalCorrectors 0;

deltaT          1;

endTime         1;

writeInterval   1;

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         leastSquares;
}

divSchemes
{
    default         none;
    div(rhoFlux,rho) Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


