FoamFile
{
    format          ascii;
    class           dictionary;
    location        "constant/rightSolid";
    object          properties;
}

epsilonRleftFluid 1;

epsilonRrightSolid 1;

dirichletValueLeft 10;

dirichletValueRight 1;

laplacianScheme Gauss linear corrected;

solver          PCG;

smoother        GaussSeidel;

preconditioner  DIC;

tolerance       1e-08;

relTol          0;

nNonOrthogonalCorrectors 0;

deltaT          0.1;

endTime         1;

writeInterval   1;

dielectricConstant 1;


