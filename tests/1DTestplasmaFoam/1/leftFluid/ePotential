FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "1/leftFluid";
    object      ePotential;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   nonuniform List<scalar> 5(37.7852 93.3557 148.926 204.497 260.067);

boundaryField
{
    bottomTopBackFront
    {
        type            empty;
    }
    left
    {
        type            fixedValue;
        value           uniform 10;
    }
    leftFluid_to_rightSolid
    {
        type            coupledElectricPotential;
        useImplicit     true;
        refValue        uniform 259.167;
        refGradient     uniform 112.941;
        valueFraction   uniform 0.5;
        source          uniform 4.96016e-10;
        value           uniform 287.852;
        surfCharge      surfCharge;
        logInterval     -1;
        writePrecision  6;
        updateHeader    1;
        writeToFile     0;
        useUserTime     1;
    }
}


