FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.85/rightSolid";
    object      ePotential;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   nonuniform List<scalar> 3(86.2057 86.2057 86.2057);

boundaryField
{
    backFront
    {
        type            empty;
    }
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    right
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    rightSolid_to_leftFluid
    {
        type            coupledElectricPotential;
        useImplicit     true;
        refValue        nonuniform List<scalar> 3(87.2057 87.2057 87.2057);
        refGradient     uniform 22.5882;
        valueFraction   uniform 0.166667;
        source          nonuniform List<scalar> 3(8.33333e-10 8.33333e-10 8.33333e-10);
        value           nonuniform List<scalar> 3(95.7841 95.7841 95.7841);
        surfChargeNbr   surfCharge;
        logInterval     -1;
        writePrecision  6;
        updateHeader    1;
        writeToFile     0;
        useUserTime     1;
    }
    procBoundary0to1
    {
        type            processor;
        value           nonuniform List<scalar> 4(86.2057 67.0489 67.0489 67.0489);
    }
}


