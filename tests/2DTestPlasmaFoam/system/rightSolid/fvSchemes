// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
FoamFile
{
    format          ascii;
    class           dictionary;
    location        "system/rightSolid";
    object          fvSchemes;
}

epsilonRleftFluid 1;

epsilonRrightSolid 5;

dirichletValueLeft 10;

dirichletValueRight 1;

surfChargeValue 1e-09;

laplacianScheme Gauss linear corrected;

solver          PCG;

smoother        GaussSeidel;

preconditioner  DIC;

tolerance       1e-08;

relTol          0;

nNonOrthogonalCorrectors 0;

deltaT          0.05;

endTime         1;

writeInterval   1;

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         leastSquares;
}

divSchemes
{
    default         none;
    div(rhoFlux,rho) Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


