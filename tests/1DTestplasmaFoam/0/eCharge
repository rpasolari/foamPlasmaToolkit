FoamFile
{
    format      ascii;
    class       volScalarField;
    object      eCharge;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    bottomTopBackFront
    {
        type            empty;
    }

    left
    {
        type            zeroGradient;
    }

    right
    {
        type            zeroGradient;
    }
}
