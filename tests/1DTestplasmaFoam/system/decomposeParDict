FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  3;

method  scotch;

constraints
{
    faces
    {
        type    preserveFaceZones;
        zones
        (
            leftFluid_to_rightSolid
            rightSolid_to_leftFluid
        );
        enabled true;
    }
}

