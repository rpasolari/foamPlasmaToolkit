FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       regIOobject;
    location    "constant/polyMesh";
    object      cellZones;
    meta
    {
        names           2 ( leftFluid rightSolid );
    }
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

2
(leftFluid
{
    type            cellZone;
    cellLabels      List<label> 5(0 1 2 3 4);
}
rightSolid
{
    type            cellZone;
    cellLabels      List<label> 5(5 6 7 8 9);
}
)

