FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/domain2";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    back
    {
        type            empty;
    }
    down
    {
        type            zeroGradient;
    }
    up
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    right
    {
        type            zeroGradient;
    }
    domain2_to_domain1
    {
        type            calculated;
        value           uniform 0;
    }
}


