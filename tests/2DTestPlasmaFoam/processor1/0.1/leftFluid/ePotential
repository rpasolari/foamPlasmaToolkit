FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.1/leftFluid";
    object      ePotential;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   nonuniform List<scalar> 3(70.0489 87.2057 87.2057);

boundaryField
{
    backFront
    {
        type            empty;
    }
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    left
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    leftFluid_to_rightSolid
    {
        type            coupledElectricPotential;
        useImplicit     true;
        refValue        nonuniform List<scalar> 2(86.2057 86.2057);
        refGradient     uniform 112.941;
        valueFraction   uniform 0.833333;
        source          uniform 1.66667e-10;
        value           nonuniform List<scalar> 2(95.7841 95.7841);
        surfCharge      surfCharge;
        logInterval     -1;
        writePrecision  6;
        updateHeader    1;
        writeToFile     0;
        useUserTime     1;
    }
    procBoundary1to0
    {
        type            processor;
        value           nonuniform List<scalar> 4(52.892 70.0489 70.0489 87.2057);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 87.2057;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 70.0489;
    }
}


