FoamFile
{
    format      ascii;
    class       volScalarField;
    object      eCharge;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type    fixedValue;
        value   uniform 0;
    }

    walls
    {
        type    fixedValue;
        value   uniform 0;
    }

    frontAndBack
    {
        type    empty;
    }
}
